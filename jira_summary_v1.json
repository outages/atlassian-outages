      "resolved_at": null,
      "impact": "maintenance",
      "shortlink": "https://stspg.io/b40slc8y85z4",
      "started_at": "2025-03-17T10:53:03.278Z",
      "page_id": "7yh3h3y0c0x1",
      "incident_updates": [
        {
          "id": "bgcn2kt73bhn",
          "status": "in_progress",
          "body": "We understand some customers may have questions regarding the ongoing maintenance for Jira and Confluence. To clarify, the current maintenance impacting Free edition users in Singapore and Northern California during regular business hours does not affect Standard, Premium, or Enterprise customers.\nFor all Standard, Premium, and Enterprise customers in all regions, Atlassian will continue to utilize established maintenance windows for any infrastructure maintenance activities.",
          "incident_id": "13yfkv7c6xd5",
          "created_at": "2025-04-15T08:35:51.674Z",
          "updated_at": "2025-04-15T08:35:51.674Z",
          "display_at": "2025-04-15T08:35:51.674Z",
          "affected_components": null,
          "deliver_notifications": false,
          "custom_tweet": null,
          "tweet_id": null
        },
        {
          "id": "rkq2nk0fdjk7",
          "status": "in_progress",
          "body": "Scheduled maintenance is currently in progress. We will provide updates as necessary.",
          "incident_id": "13yfkv7c6xd5",
          "created_at": "2025-03-18T00:00:21.730Z",
          "updated_at": "2025-03-18T00:00:21.730Z",
