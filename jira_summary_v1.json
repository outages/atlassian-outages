      "impact": "major",
      "shortlink": "https://stspg.io/r2lptm3c01kk",
      "started_at": "2023-08-15T10:46:01.704Z",
      "page_id": "7yh3h3y0c0x1",
      "incident_updates": [
        {
          "id": "05c0fgw6zwf5",
          "status": "monitoring",
          "body": "We have identified the root cause of the group deletions and have mitigated the problem. We are now monitoring closely. \n\nIf this believe the issue has not been resolved for you, please don't hesitate to get in touch with Atlassian Support at https://support.atlassian.com/contact/",
          "incident_id": "gt1rv2vs56yx",
          "created_at": "2023-08-17T08:44:09.787Z",
          "updated_at": "2023-08-17T08:44:09.787Z",
