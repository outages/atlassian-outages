    {
      "id": "3j0168lmh5j3",
      "name": "Forge Hosted Storage Scheduled Data Residency Rollout",
      "status": "in_progress",
      "created_at": "2024-03-27T10:32:18.209Z",
      "updated_at": "2024-04-24T08:08:38.136Z",
      "monitoring_at": null,
      "resolved_at": null,
      "impact": "maintenance",
      "shortlink": "https://stspg.io/w2gykbmg8jrh",
      "started_at": "2024-03-28T03:12:31.536Z",
      "page_id": "n8766lc3ybbs",
      "incident_updates": [
        {
          "id": "vv2rdxrm9vzk",
          "status": "in_progress",
          "body": "The move of the data for Forge hosted storage to the same location as the host product is taking longer than expected and will not be completed by 30 Apr 2024 00:00 UTC, as previously announced. After careful consideration, the Forge hosted storage decided that the best path forward is to extend the completion date from 30 Apr 2024 to 17 May 2024 00:00 UTC.",
          "incident_id": "3j0168lmh5j3",
          "created_at": "2024-04-24T08:08:38.134Z",
          "updated_at": "2024-04-24T08:08:38.134Z",
          "display_at": "2024-04-24T08:08:38.134Z",
          "affected_components": [
            {
              "code": "9nthl2bkwn0x",
              "name": "Developer - Hosted storage",
              "old_status": "operational",
              "new_status": "operational"
            }
          ],
          "deliver_notifications": false,
          "custom_tweet": null,
          "tweet_id": null
        },
        {
          "id": "lz0r5qm73jtg",
          "status": "in_progress",
          "body": "We have started the rollout of data residency for Forge hosted storage, where data will be moving to the same location as the host product. We expect the rollout to be completed by Apr 30, 2024 00:00 UTC.  \n\nPlease refer to https://developer.atlassian.com/platform/forge/changelog/#CHANGE-1556 for more information.",
          "incident_id": "3j0168lmh5j3",
          "created_at": "2024-03-28T03:12:31.536Z",
          "updated_at": "2024-03-28T03:29:06.692Z",
          "display_at": "2024-03-28T03:12:31.536Z",
