    {
      "id": "08g7p82qpgxp",
      "name": "Loom Users on Chrome Extension Encountering Video Saving Delays",
      "status": "identified",
      "created_at": "2025-10-08T22:20:37.356Z",
      "updated_at": "2025-10-08T22:20:37.436Z",
      "monitoring_at": null,
      "resolved_at": null,
      "impact": "minor",
      "shortlink": "https://stspg.io/c4m1g48r2sx8",
      "started_at": "2025-10-08T22:20:37.350Z",
      "page_id": "3tbw4lb9t3qp",
      "incident_updates": [
        {
          "id": "rvtqtk8b6m76",
          "status": "identified",
          "body": "We have identified an issue starting at 17:00 UTC on Monday, October 6th, 2025 affecting all Loom users who record videos with the Chrome Extension, causing unexpected delays when saving videos. If the browser is closed before the video is playable, the recording may be lost.\n\nTo address this, we have developed an updated version of the extension, which is currently under review by Google. We anticipate the fix to be published by 20:00 UTC on October 15th, 2025, or sooner. In the meantime, users will see an in-browser message if their video has not yet been saved, advising them not to close the browser session.\n\nWe will provide an update again when the fix is available.",
          "incident_id": "08g7p82qpgxp",
          "created_at": "2025-10-08T22:20:37.433Z",
          "updated_at": "2025-10-08T22:20:37.433Z",
