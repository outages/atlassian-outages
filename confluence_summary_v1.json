      "page_id": "4g2my7tbhjsq",
      "incident_updates": [
        {
          "id": "t7k09gxd7vt9",
          "status": "identified",
          "body": "We have partially mitigated the issue - the condition in which user is being prompted continuously for consent and unable to use app is now less likely to happen but still possible.\r\n\r\nThe incident team is investigating this problem further to find root cause of remaining source of reliability problems.",
          "incident_id": "r334c9tzp91m",
          "created_at": "2022-07-18T18:39:19.540Z",
          "updated_at": "2022-07-18T18:39:19.540Z",
