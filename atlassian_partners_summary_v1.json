      "incident_updates": [
        {
          "id": "hwl2yds129h4",
          "status": "monitoring",
          "body": "The issues causing the unavailability of related Forge apps capabilities on multiple Atlassian platforms have been resolved, and services are now operating normally for all impacted customers. We will continue to closely monitor for stability.",
          "incident_id": "grb2gdfkn3zf",
          "created_at": "2025-04-28T13:42:35.672Z",
          "updated_at": "2025-04-28T13:42:35.672Z",
