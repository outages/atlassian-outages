    {
      "id": "75c5xgk7gj4q",
      "name": "2FA, Password reset, OTP, and verification emails being quarantined",
      "status": "monitoring",
      "created_at": "2025-11-13T18:56:12.844Z",
      "updated_at": "2025-11-14T04:57:36.854Z",
      "monitoring_at": "2025-11-14T04:57:36.834Z",
      "resolved_at": null,
      "impact": "none",
      "shortlink": "https://stspg.io/xg512x20wxw7",
      "started_at": "2025-11-13T18:56:12.825Z",
      "page_id": "5qmgwsxwsz78",
      "incident_updates": [
        {
          "id": "pzry3g39jnl1",
          "status": "monitoring",
          "body": "Our investigation has indicated that the issues with email delivery are related to quarantine rules in effect within our customers email service providers.\n\nFor customers that are continuing to experience this issue, we encourage you to reach out to your organizations email administrator to review emails placed into quarantine and to release those emails.",
